    <!-- JS: Live Day Date Time -->
    <script type="text/javascript">

        // function to set live day, date and time
        setInterval(function () {
            updateDateTime();
        }, 1000);

        function updateDateTime() {

            var now = new Date();

            var day = now.toLocaleDateString('en-US', {
                weekday: 'long'
            });

            var month = now.toLocaleDateString('en-US', {
                month: 'long'
            });

            var date = now.getDate();
            var year = now.getFullYear();

            var time = now.toLocaleTimeString('en-US', {
                hour: '2-digit',
                minute: '2-digit',
                second: '2-digit',
                hour12: true
            });

            var formattedDateTime = day + ', ' + month + ' ' + date + ', ' + year + ' | ' + time;

            document.getElementById('<%= lblLiveDayDateTime.ClientID %>').innerHTML = formattedDateTime;
        }

        // open parent dropdown
        let iconLinks = document.querySelectorAll(".iocn-link");
        for (let i = 0; i < iconLinks.length; i++) {
            iconLinks[i].addEventListener("click", (e) => {
                let iconLinkParent = e.currentTarget.parentElement; // selecting main parent of iconLink
                iconLinkParent.classList.toggle("showMenu");
            });
        }

        // open parent dropdown
        let arrows = document.querySelectorAll(".arrow");
        for (let i = 0; i < arrows.length; i++) {
            arrows[i].addEventListener("click", (e) => {
                e.stopPropagation(); // prevent the event from bubbling up to the parent div
                let arrowParent = e.target.parentElement.parentElement; // selecting main parent of arrow
                arrowParent.classList.toggle("showMenu");
            });
        }


        // toggle side bar to open and close
        let sidebar = document.querySelector(".sidebar");
        let sidebarBtn = document.querySelector(".bx-menu");
        console.log(sidebarBtn);
        sidebarBtn.addEventListener("click", () => {
            sidebar.classList.toggle("close");
        });
    </script>


    <!-- iFrame Modal Popup Window JavaScript -->
    <script>
        // Function to open the modal, load a page into the iframe, and set the size dynamically
        function openModal(pageUrl, width = '60%', height = '80%') {
            // Set the modal's width and height based on parameters
            var modal = document.getElementById('myModal');
            modal.style.width = width;
            modal.style.height = height;
            modal.style.left = 'calc((100% - ' + width + ') / 2)'; // Center the modal horizontally

            // Set iframe source and show modal
            document.getElementById('iframeModal').src = pageUrl;
            modal.style.display = 'block';
        }

        // Close the modal when the close button is clicked
        document.getElementById('closeModal').onclick = function () {
            var modal = document.getElementById('myModal');
            modal.style.display = 'none';
            document.getElementById('iframeModal').src = ''; // Clear iframe content when closing
        }

        // Function to close the modal
        function closeModal() {
            document.getElementById('myModal').style.display = 'none';
            document.getElementById('iframeModal').src = '';
        }
    </script>


    <!-- JS: jQuery Plugin Initilization -->
    <script type="text/javascript">

        function Bind_Dropdown_By_ID(dropdownID) {
            $(dropdownID).chosen({
                disable_search: false, // disable search bar
                search_contains: false, // search characters anywhere instead of right-left
                no_results_text: "No results match", // if searched not matched
                placeholder_text_single: "Select Here......", // search bar place holder 
                allow_single_deselect: true, // clear selected item
                width: "100%", // width of element
                rtl: false, // text appears on right side right-to-left
            });
        }

        function Bind_Dropdown_By_Class(dropdownClass) {
            $(dropdownClass).chosen({
                disable_search: false, // disable search bar
                search_contains: false, // search characters anywhere instead of right-left
                no_results_text: "No results match", // if searched not matched
                placeholder_text_single: "Select Here......", // search bar placeholder
                allow_single_deselect: true, // clear selected item
                width: "100%", // width of element
                rtl: false, // text appears on right side right-to-left
            });
        }

        function Bind_Dropdown_Multicheck_By_ID(dropdownID) {
            $(dropdownID).on('sumo:opening', function () {
                $('.select-all').css('height', '40px');
            });

            $(dropdownID).SumoSelect({
                search: true,
                searchText: "search here....",
                multiSelect: true,
                okCancelInMulti: true,
                prefix: "",
                up: false, // list at top side
                selectAll: true, // select all option
                clearAll: true, // not working at all
                renderOption: function (data, escape) {
                    // Check if the option is the "Select Values" item
                    if (data.value === "0") {
                        // Add a class to "Select Values" item to handle differently
                        return '<div class="title special-item">' + escape(data.text) + '</div>';
                    } else {
                        return '<div><label><input type="checkbox" />' + escape(data.text) + '</label></div>';
                    }
                }
            });
            $(dropdownID).on('sumo:opened', function () {
                // Check if the "Select Values" item is present
                if ($(dropdownID + ' .special-item').length > 0) {
                    // Disable multiselect for the "Select Values" item
                    $(dropdownID + ' .special-item input').prop('disabled', true);
                }
            });
        }

        function Bind_Dropdown_Multicheck_By_Class(dropdownClass) {
            $(dropdownClass).on('sumo:opening', function () {
                $('.select-all').css('height', '40px');
            });

            $(dropdownClass).SumoSelect({
                search: true,
                searchText: "search here....",
                multiSelect: true,
                okCancelInMulti: true,
                prefix: "",
                up: false, // list at top side
                selectAll: true, // select all option
                clearAll: true, // not working at all
                renderOption: function (data, escape) {
                    // Check if the option is the "Select Values" item
                    if (data.value === "0") {
                        // Add a class to "Select Values" item to handle differently
                        return '<div class="title special-item">' + escape(data.text) + '</div>';
                    } else {
                        return '<div><label><input type="checkbox" />' + escape(data.text) + '</label></div>';
                    }
                }
            });

            $(dropdownClass).on('sumo:opened', function () {
                // Check if the "Select Values" item is present
                if ($(dropdownClass + ' .special-item').length > 0) {
                    // Disable multiselect for the "Select Values" item
                    $(dropdownClass + ' .special-item input').prop('disabled', true);
                }
            });
        }

        function Initialize_DataTables_By_ID(GridView_ID) {
            $(GridView_ID).each(function () {
