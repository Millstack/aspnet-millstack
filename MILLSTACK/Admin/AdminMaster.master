        function Initialize_DataTables_By_Class(gridViewClass) {
            $(gridViewClass).each(function () {
                var $this = $(this);

                // Destroy existing instance if any
                if ($this.hasClass('dataTable')) {
                    $this.DataTable().destroy();
                    $this.find('thead').remove(); // Clean up thead added by previous DataTable instance
                }

                // Add thead if it doesn't exist
                if ($this.find('thead').length === 0) {
                    $this.prepend($("<thead></thead>").append($this.find("tr:first")));
                }

                // Initialize DataTable
                $this.DataTable({
                    scrollX: false,
                    sScrollXInner: "100%",
                    bFilter: true,
                    bSort: true,
                    bPaginate: true,
                    scrollCollapse: false,
                    paging: true,
                    searching: true,
                    ordering: true,
                    info: true,
                    lengthChange: true,
                    responsive: true,
