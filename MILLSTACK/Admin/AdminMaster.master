                    ],
                    search: {
                        return: false
                    },
                    language: {
                        search: "Search: ",
                        decimal: ',',
                        thousands: '.'
                    },
                    initComplete: function () {
                        $('.dataTables_filter input').attr('placeholder', 'Search here......');
                    },
                    dom: '<"top"lfB>rt<"bottom"ip><"clear">',
                    buttons: [
                        {
                            extend: 'copy',
                            title: 'Data',
                            filename: 'Data_Copy'
                        },
                        {
                            extend: 'csv',
                            title: 'CSV Data',
                            filename: 'CSV_Data'
                        },
                        {
                            extend: 'excel',
                            title: 'Excel Data',
                            filename: 'Excel_Data'
                        },
                        {
                            extend: 'pdf',
                            title: 'PDF Data',
                            filename: 'PDF_Data'
                        },
                        {
                            extend: 'print',
                            title: 'Print_Data'
                        }
                    ]
                });
            });
        }

        $(document).ready(function () {

